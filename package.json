{
  "name": "react-uforms",
  "version": "3.6.0",
  "description": "Simple and elegant forms for your React application",
  "keywords": [
    "react",
    "form",
    "uform",
    "react-uform",
    "react form",
    "react forms",
    "typescirpt"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/bilberrry/react-uforms.git",
  "author": {
    "name": "Bohdan Protsiuk",
    "email": "bohdan.protsiuk@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "clean": "rm -rf dist && mkdir dist",
    "build:declaration": "tsc --emitDeclarationOnly --declaration --outDir dist --p tsconfig-build.json",
    "build:ts": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx\"",
    "build": "yarn clean && yarn build:ts && yarn build:declaration",
    "start": "yarn build:ts --watch",
    "test": "yarn jest src",
    "test-coverage": "yarn test --coverage",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0",
    "yup": "^0.32.11"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.transform": "^4.6.0",
    "lodash.xorwith": "^4.5.0",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.1",
    "@types/lodash.clonedeep": "^4.5.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.transform": "^4.6.6",
    "@types/lodash.xorwith": "^4.5.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "tsc": "^2.0.3",
    "typescript": "^4.8.4",
    "yup": "^0.32.11"
  }
}
