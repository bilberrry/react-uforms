{
  "name": "react-uforms",
  "version": "2.4.5",
  "description": "Simple and elegant forms for your React application",
  "keywords": [
    "react",
    "form",
    "uform",
    "react-uform",
    "react form",
    "react forms",
    "typescirpt"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "http://react-uforms.d3v.me/",
  "repository": "https://github.com/summerua/react-uforms.git",
  "author": {
    "name": "Bohdan Protsiuk",
    "email": "bohdan.protsiuk@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "clean": "rm -rf dist && mkdir dist",
    "build:declaration": "tsc --emitDeclarationOnly --declaration --outDir dist --p tsconfig-build.json",
    "build:ts": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx\"",
    "build": "yarn clean && yarn build:ts && yarn build:declaration",
    "start": "yarn build:ts --watch",
    "test": "jest",
    "test-coverage": "yarn test --coverage"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.2"
  }
}
