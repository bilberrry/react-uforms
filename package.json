{
  "name": "react-uforms",
  "version": "2.8.1",
  "description": "Simple and elegant forms for your React application",
  "keywords": [
    "react",
    "form",
    "uform",
    "react-uform",
    "react form",
    "react forms",
    "typescirpt"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "http://react-uforms.d3v.me/",
  "repository": "https://github.com/summerua/react-uforms.git",
  "author": {
    "name": "Bohdan Protsiuk",
    "email": "bohdan.protsiuk@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "clean": "rm -rf dist && mkdir dist",
    "build:declaration": "tsc --emitDeclarationOnly --declaration --outDir dist --p tsconfig-build.json",
    "build:ts": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx\"",
    "build": "yarn clean && yarn build:ts && yarn build:declaration",
    "start": "yarn build:ts --watch",
    "test": "jest",
    "test-coverage": "yarn test --coverage"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.transform": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^28.1.6",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.set": "^4.3.7",
    "@types/lodash.transform": "^4.6.7",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.2.1",
    "ts-jest": "^28.0.7",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  }
}
